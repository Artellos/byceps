location / {
    try_files $uri @byceps;
}

location = /favicon.ico {
    return 404;
}

location ^~ /static/ {
    alias /var/www/byceps/byceps/static/;
}

location ~ ^/static_sites/(.+?)/(.+)$ {
    alias /var/www/byceps/sites/$1/static/$2;
}

location ^~ /data/ {
    alias /var/www/byceps/data/;
}
